<view class="content data-v-03c52bbe"><view class="title data-v-03c52bbe"><image class="logo data-v-03c52bbe" src="/static/logo.png"></image><text class="data-v-03c52bbe">Todos on wechat</text></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="addBtn data-v-03c52bbe" bindtap="__e">Add new todo</view><view class="todoList data-v-03c52bbe"><block wx:if="{{todoList.length==0}}"><view style="text-align:center;" class="data-v-03c52bbe">You have nothing todo</view></block><block wx:else><view class="data-v-03c52bbe"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['finishTodo',[index]]]]]}}" class="todoItem data-v-03c52bbe" bindtap="__e"><view class="index data-v-03c52bbe">{{''+(index+1)+''}}</view><view class="item data-v-03c52bbe"><view class="time data-v-03c52bbe">{{"创建时间:"+item.m0}}</view><view class="msg data-v-03c52bbe" style="{{(item.$orig.finished?'text-decoration-line: line-through; color: #555555;':'')}}">{{''+item.$orig.msg+''}}</view></view></view></block></view></block></view><view hidden="{{!(showed)}}" class="addPanel data-v-03c52bbe"><view class="panel data-v-03c52bbe"><view class="panelTitle data-v-03c52bbe">Add a new todo</view><textarea data-event-opts="{{[['input',[['__set_model',['','msg','$event',[]]]]]]}}" value="{{msg}}" bindinput="__e" class="data-v-03c52bbe"></textarea><button type="primary" data-event-opts="{{[['tap',[['addNewTodo']]]]}}" bindtap="__e" class="data-v-03c52bbe">确定</button></view></view></view>